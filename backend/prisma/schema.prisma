generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Question {
  id           String   @id @default(uuid())
  group        String
  title        String   @unique
  firsrtAnswer String
  secondAnswer String
  thirdAnswer  String
  fourthAnswer String
  CorectAnswer String
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt

  @@map("questions")
}
